pipeline {
    agent any
    
     environment{
        def dockerHome = tool 'myDocker'
        env.PATH = "${dockerHome}/bin:${env.PATH}"
     }
    
    stages   {
        stage ('Copy from Container to Host '){
            agent {docker 'pamsergio/jenkinsapp'}
            steps {
                echo 'Copy files from recent container to local machine'
                sh 'node --version'
            }
        }
    }
} 
